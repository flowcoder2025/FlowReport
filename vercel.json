{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/sync",
      "schedule": "0 18 * * *"
    },
    {
      "path": "/api/cron/snapshots",
      "schedule": "0 0 * * 1"
    },
    {
      "path": "/api/cron/reports",
      "schedule": "0 0 * * *"
    }
  ]
}
